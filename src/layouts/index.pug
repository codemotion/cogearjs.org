doctype html
html(lang="en")
	head
		title= cogear.config.title
		include includes/header
		block head
	body
		.navbar#nav(role="navigation" aria-label="main navigation")
			.navbar-brand 
					a.navbar-item.logo(href="/") COGEAR.JS
					a.navbar-burger(role="button" aria-label="menu" aria-expanded="false" data-target="menu")
						span(aria-hidden="true")
						span(aria-hidden="true")
						span(aria-hidden="true")
			.navbar-menu.animated.faster#menu
				.navbar-start
				.navbar-end
					.navbar-item: a(href="/docs") Docs
					.navbar-item: a(href="/blog") Blog
					.navbar-item: a(href="/video") Video
					.navbar-item: a(href="https://github.com/codemotion/cogear.js") Github
					.navbar-item: a(href="https://github.com/codemotion/cogear.js/issues") Issues
					.navbar-item: a(href="/docs/license") License
					.navbar-item#version v1.0.0
				
		block content
			!= content
		include includes/footer